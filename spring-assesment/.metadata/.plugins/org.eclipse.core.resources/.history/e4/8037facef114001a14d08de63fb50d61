package com.ustglobal.springmvc.dto;

import javax.persistence.Table;

@Table(name="product")
public class Product {

	private int oid;
	private int pid;
	private int vid;
	public int getOid() {
		return oid;
	}
	public void setOid(int oid) {
		this.oid = oid;
	}
	public int getPid() {
		return pid;
	}
	public void setPid(int pid) {
		this.pid = pid;
	}
	public int getVid() {
		return vid;
	}
	public void setVid(int vid) {
		this.vid = vid;
	}
	
	
	
}
